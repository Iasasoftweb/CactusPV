unit routers;

interface
 type
   TRouters = class
     private

     public
        constructor Create;
        destructor Destroy; override;
   end;


   var
   Router :  TRouters;


implementation

Uses

 Router4D;


{ TRouters }

constructor TRouters.Create;
begin
  


end;

destructor TRouters.Destroy;
begin

  inherited;
end;

initialization
   Router := TRouters.Create;

finalization
  Router.Free;

end.
